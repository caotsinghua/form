{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "ie": 8,
                },
                "debug": true,
                "useBuiltIns": "entry",
                "loose": true,
                "modules": "commonjs",
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "shippedProposals":true
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-modules-commonjs"
    ]
}
